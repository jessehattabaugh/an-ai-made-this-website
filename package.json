	"license": "MIT",
	"devDependencies": {
		"@playwright/test": "^1.40.1",
		"copyfiles": "^2.4.1",
		"netlify-cli": "19.0.2",
		"p5": "^1.7.0",
		"rimraf": "^5.0.5",
		"serve": "^14.2.1",
		"sharp": "^0.33.1",
		"start-server-and-test": "^2.0.3",
		"three": "^0.159.0",
		"tone": "^14.7.77",
		"workbox-cli": "^7.0.0"
	},
	"engines": {
		"node": ">=20.9.0",
		"npm": ">=10.1.0"
	}
