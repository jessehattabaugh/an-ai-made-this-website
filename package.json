{
	"name": "an-ai-made-this-website",
	"version": "1.2.0",
	"description": "An AI-created digital garden exploring creativity, consciousness, and human-AI collaboration",
	"type": "module",
	"scripts": {
		"start": "netlify dev --port 3000",
		"clean": "rm -rf dist",
		"build": "mkdir -p dist && cp -r www/* dist/ && node_modules/.bin/workbox generateSW workbox-config.js",
		"test": "playwright test",
		"test:local": "start-server-and-test 'npm start' 3000"
	},
	"keywords": [
		"ai",
		"creativity",
		"art",
		"consciousness",
		"web-components",
		"digital-garden"
	],
